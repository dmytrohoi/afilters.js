{
  "name": "afilters.js",
  "version": "1.0.0",
  "description": "Library to create advanced filters",
  "main": "./src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./utils": "./src/utils.js"
  },
  "scripts": {
    "test": "jest",
    "test:codecov": "npm test && codecov",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prettier": "prettier --write src/**/*.js",
    "prepublishOnly": "npm run test",
    "doc": "jsdoc --output=doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dmytrohoi/afilters.js.git"
  },
  "keywords": [
    "filters",
    "api",
    "bot",
    "client",
    "node"
  ],
  "author": "Dmytro Hoi <code@dmytrohoi.com> (http://dmytrohoi.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dmytrohoi/afilters.js/issues"
  },
  "homepage": "https://github.com/dmytrohoi/afilters.js#readme",
  "runkitExampleFilename": "./examples/filter.js",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "codecov": "^3.8.1",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.6.0",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "all",
    "endOfLine": "lf",
    "arrowParens": "avoid"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "**/src/*.js",
      "!**/errors.js",
      "!**/node_modules/**",
      "!**/test/**"
    ]
  }
}
